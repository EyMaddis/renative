{
    "TODO": "This file will be migrated to separate engines in the future",
    "engines": {
        "engine-rn": {
            "id": "engine-rn",
            "overview": "Default react-native based engine. Runs standard react native apps with native support",
            "requiresMetroConfig": true,
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "@react-native-community/cli-platform-ios": "source:rnv",
                "@react-native-community/cli": "source:rnv"
            },
            "platforms": {
                "ios": {
                    "engine": "engine-rn",
                    "npm": {
                        "devDependencies": {
                            "xcode": "2.1.0"
                        },
                        "optionalDependencies": {
                            "ios-deploy": "1.10.0"
                        }
                    }
                },
                "android": {
                    "engine": "engine-rn",
                    "npm": {
                        "devDependencies": {
                            "jetifier": "1.6.5"
                        }
                    }
                },
                "androidwear": {
                    "npm": {
                        "devDependencies": {
                            "jetifier": "1.6.5"
                        }
                    }
                },
                "androidtv": {
                    "npm": {
                        "devDependencies": {
                            "jetifier": "1.6.5"
                        }
                    }
                },
                "tvos": {
                    "npm": {
                        "devDependencies": {
                            "xcode": "2.1.0"
                        },
                        "optionalDependencies": {
                            "ios-deploy": "1.10.0"
                        }
                    }
                }
            }
        },
        "engine-rn-web": {
            "id": "engine-rn-web",
            "overview": "React native based engine with web transpiler provided by react-native-web",
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "@react-native-community/cli-platform-ios": "source:rnv",
                "@react-native-community/cli": "source:rnv"
            },
            "platforms": {
                "tizen": {
                    "npm": {
                        "dependencies": {
                            "raf": "3.4.1",
                            "html-webpack-harddisk-plugin": "0.2.0",
                            "html-webpack-plugin": "3.2.0"
                        }
                    }
                },
                "web": {
                    "npm": {
                        "dependencies": {
                            "html-webpack-harddisk-plugin": "0.2.0",
                            "html-webpack-plugin": "3.2.0"
                        }
                    }
                }
            }
        },
        "engine-rn-electron": {
            "id": "engine-rn-electron",
            "overview": "React native based engine with web transpiler provided by react-native-web and native desktop wrapper provided by electron",
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "@react-native-community/cli-platform-ios": "source:rnv",
                "@react-native-community/cli": "source:rnv"
            },
            "platforms": {
                "macos": {
                    "npm": {
                        "devDependencies": {
                            "electron": "7.0.0",
                            "electron-builder": "21.1.1"
                        }
                    }
                },
                "windows": {
                    "npm": {
                        "devDependencies": {
                            "electron": "7.0.0",
                            "electron-builder": "21.1.1"
                        }
                    }
                }
            }
        },
        "engine-rn-next": {
            "id": "engine-rn-next",
            "overview": "React native based engine with web transpiler provided by react-native-web and render by nextjs",
            "plugins": {
                "react": "source:rnv",
                "react-art": "source:rnv",
                "react-dom": "source:rnv",
                "react-native": "source:rnv",
                "react-native-web": "source:rnv",
                "next": "source:rnv"
            },
            "platforms": {
                "web": {
                    "npm": {
                        "dependencies": {
                            "html-webpack-harddisk-plugin": "0.2.0",
                            "html-webpack-plugin": "3.2.0",
                            "@zeit/next-css": "1.0.1",
                            "next": "9.4.3",
                            "next-routes": "1.4.2",
                            "@expo/next-adapter": "2.0.5",
                            "next-compose-plugins": "^2.2.0",
                            "next-fonts": "^1.0.3",
                            "next-images": "^1.3.1",
                            "babel-preset-expo": "^8.0.0",
                            "next-transpile-modules": "^3.0.2"
                        }
                    }
                },
                "web-next": {
                    "npm": {
                        "dependencies": {
                            "html-webpack-harddisk-plugin": "0.2.0",
                            "html-webpack-plugin": "3.2.0",
                            "@zeit/next-css": "1.0.1",
                            "next": "9.4.3",
                            "next-routes": "1.4.2",
                            "@expo/next-adapter": "2.0.5",
                            "next-compose-plugins": "^2.2.0",
                            "next-fonts": "^1.0.3",
                            "next-images": "^1.3.1",
                            "babel-preset-expo": "^8.0.0",
                            "next-transpile-modules": "^3.0.2"
                        }
                    }
                }
            }
        }
    }
}
